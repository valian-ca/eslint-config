{
  "name": "@valian/eslint-config",
  "description": "Valian ESLint Config",
  "version": "1.4.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valian-ca/eslint-config.git"
  },
  "author": {
    "name": "Valian",
    "url": "https://valian.ca",
    "organization": true
  },
  "keywords": [
    "eslint"
  ],
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "eslint": "eslint .",
    "prettier": "prettier --check .",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "bump": "npx changelogen@latest --release --bump",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "eslint": "9.20.0",
    "husky": "^9.1.7",
    "markdownlint-cli2": "0.17.2",
    "prettier": "^3.5.0",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "eslint": ">=9.20.0",
    "typescript": ">=5.7.3"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.23.0"
  },
  "engines": {
    "node": ">= 20"
  },
  "exports": {
    ".": "./index.mjs",
    "./base": "./rules/base.mjs",
    "./importSort": "./rules/importSort.mjs",
    "./no-cycle": "./rules/no-cycle.mjs",
    "./jest": "./rules/jest.mjs",
    "./node": "./rules/node.mjs",
    "./react": "./rules/react.mjs",
    "./typescript": "./rules/typescript.mjs"
  },
  "type": "module",
  "main": "index.mjs",
  "module": "./index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.6.2"
}
