{
  "name": "@valian/eslint-config",
  "description": "Valian ESLint Config",
  "version": "2.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valian-ca/eslint-config.git"
  },
  "author": {
    "name": "Valian",
    "url": "https://valian.ca",
    "organization": true
  },
  "keywords": [
    "eslint"
  ],
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "eslint": "eslint .",
    "prettier": "prettier --check .",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "bump": "npx changelogen@latest --release --bump --push --clean",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "publint": "^0.3.15",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": ">=9.35.0",
    "eslint-plugin-storybook": ">=9.0.0",
    "typescript": ">=5.8.3"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-storybook": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@eslint/json": "^0.14.0",
    "@stylistic/eslint-plugin": "^5.6.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/eslint-plugin": "^1.4.3",
    "confusing-browser-globals": "^1.0.11",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-use-extend-native": "^0.7.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-zod-x": "^1.9.0",
    "globals": "^16.5.0",
    "typescript-eslint": "^8.47.0"
  },
  "engines": {
    "node": ">= 20"
  },
  "exports": {
    ".": "./index.mjs",
    "./base": "./rules/base.mjs",
    "./import-sort": "./rules/import-sort.mjs",
    "./no-cycle": "./rules/no-cycle.mjs",
    "./json": "./rules/json.mjs",
    "./jest": "./rules/jest.mjs",
    "./node": "./rules/node.mjs",
    "./react": "./rules/react.mjs",
    "./storybook": "./rules/storybook.mjs",
    "./typescript": "./rules/typescript.mjs",
    "./vitest": "./rules/vitest.mjs",
    "./zod": "./rules/zod.mjs"
  },
  "type": "module",
  "main": "index.mjs",
  "module": "./index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.22.0"
}
